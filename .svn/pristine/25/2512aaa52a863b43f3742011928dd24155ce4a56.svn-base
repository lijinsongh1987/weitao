	<script>
		$(function() {
			laydate.skin('molv');
			laydate({
				elem : '#starttime',
				format : 'YYYY-MM-DD', // 分隔符可以任意定义，该例子表示只显示年月
				festival : true, //显示节日
			});
			laydate({
				elem : '#endtime',
				format : 'YYYY-MM-DD', // 分隔符可以任意定义，该例子表示只显示年月
				festival : true, //显示节日
			});
			$("[data-rel='popover']").popover();
			$("[data-rel='popover']").on("mouseenter", function () {
	                    var _this = this;
	                    $(this).popover("show");
	                    $(this).siblings(".popover").on("mouseleave", function () {
	                        $(_this).popover('hide');
	                    });
	                }).on("mouseleave", function () {
	                    var _this = this;
	                    setTimeout(function () {
	                        if (!$(".popover:hover").length) {
	                            $(_this).popover("hide")
	                        }
	                    }, 100);
	                });
		});
		window.onload=function(){
		     var CheckAll=document.getElementById('chose-all');
		     var UnCheck=document.getElementById('chose-none');
		     var OtherCheck=document.getElementById('chose-reverse');
		     var CheckBox=document.getElementsByName('goods-chose');
		     CheckAll.onclick=function(){
		             for(i=0;i<CheckBox.length;i++){
		                     CheckBox[i].checked=true;
		                 };
		         };
		     UnCheck.onclick=function(){
		             for(i=0;i<CheckBox.length;i++){
		                     CheckBox[i].checked=false;
		                 };
		         };
		     OtherCheck.onclick=function(){
		             for(i=0;i<CheckBox.length;i++){
		                     if(CheckBox[i].checked==true){
		                             CheckBox[i].checked=false;
		                         }
		                     else{
		                         CheckBox[i].checked=true;
		                        }
		                     
		                 };
		         };
		};
		function tip(obj) {
			var image = $(obj).attr('data');
			$index = layer
					.tips(
							'<img style="width:300px; height:300px;" src="'+image+'"/>',
							obj, {
								area : [ '320px', '310px' ],
								tips : [ 2, '#FAFAFA' ]
							});
		}

		function editgoods(id,status) {
			$.ajax({
				type : "post",
				url : "?s=GoodsCenter/tuijiantrue",
				data : {"id":id,"status":status},
				dataType : "json",
				success : function(data) {
					if (data['msg'] == 1) {
						layer.msg('操作成功', {time : 1000}, function() {
							window.location.reload();
						});
					} else {
						layer.msg('操作失败', {time : 1000});
					}
				}
			});
		}
		$(function() {
			var id = "{$shaixuanid}";
			if (id == 1) {
				$("#shaixuan").css("display", "block");
				$("#shaixuanid").val("1");
			} else {
				$("#shaixuan").css("display", "none");
				$("#shaixuanid").val("0");
			}
		});
		function shaixuan() {
			$("#shaixuan").css("display");
			if ($("#shaixuan").css("display") == "none") {
				$("#shaixuan").css("display", "block");
				$("#shaixuanid").val("1");
			} else {
				$("#shaixuan").css("display", "none");
				$("#shaixuanid").val("0");
			}
		}
		function batheditgoods(status){
			if (!confirm("确定批量操作所有勾选的商品?")) {
	            return;
	        }
			var ids = "";
			var CheckBox=document.getElementsByName('goods-chose');
		    for(var i = 0; i < CheckBox.length; i++){
			     if(CheckBox[i].checked){
			    	 if (i < (CheckBox.length - 1)) {
			    		 ids += CheckBox[i].value+",";
			    	 } else if (i == (CheckBox.length - 1)) {
			    		 ids += CheckBox[i].value;
			    	 }
			     }
		    } 
		    $.ajax({
				type : "post",
				url : "?s=GoodsCenter/batheditgoods",
				data : {"ids":ids,"status":status},
				dataType : "json",
				success : function(data) {
					if (data['msg'] == 1) {
						layer.msg('操作成功', {time : 1000}, function() {
							window.location.reload();
						});
					} else {
						layer.msg('操作失败', {time : 1000});
					}
				}
			});
		}
		
		function searchgoods (goodsid,dtitle,typename,orgprice,salesnum,sellerid,istmall,status,updatetime,shangjiatime,xiajiatime,quanprice,quanid,price,quanprice,quancondition,quantime,commissionjihua,commissionqueqiao,jihuashenhe,pic){
			$("#GoodsId").val(goodsid);
			$("#D_title").val(dtitle);
			$("#typename").val(typename);
			$("#Org_Price").val(orgprice);
			$("#Sales_num").val(salesnum);
			$("#SellerID").val(sellerid);
			if (istmall == 0) {
				$("#IsTmall").val("天猫商城");
			} else {
				$("#IsTmall").val("淘宝商城");
			}
			if (status == 1){
				$("#goodsstatus").val("已下架");
			} else {
				$("#goodsstatus").val("正常上架");
			}
			$("#update_time").val(updatetime);
			$("#shangjiatime").val(shangjiatime);
			$("#xiajiatime").val(xiajiatime);
			$("#Quan_receive").val(quanprice);
			$("#Quan_id").val(quanid);
			$("#Price").val(price);
			$("#Quan_price").val(quanprice);
			$("#Quan_condition").val(quancondition);
			$("#quan_update_time").val(updatetime);
			$("#Quan_time").val(quantime);
			//获取当前时间判断优惠卷是否过期
			var now = getNowFormatDate();
			var oDate1 = new Date(now);
		    var oDate2 = new Date(quantime);
		    if (oDate1.getTime() > oDate2.getTime()) {
		    	$("#Quan_status").val("已经失效");
		    } else {
		    	$("#Quan_status").val("正常发放");
		    }
		    if (commissionjihua == 0) {
		    	$("#Commissiontype").val("鹊桥计划");
		    	$("#CommissionRate").val(commissionqueqiao+"%");
		    } else if (commissionqueqiao == 0) {
		    	$("#Commissiontype").val("通用计划");
		    	$("#CommissionRate").val(commissionjihua+"%");
		    }
			if (jihuashenhe == 0) {
				$("#Jihua_shenhe").val("未知申请状态");
			} else {
				$("#Jihua_shenhe").val("");
			}
			$("#goodspic").attr("href",pic); 
			$("#goodsinfo").attr("href","https://detail.tmall.com/item.htm?id="+goodsid); 
			$("#goodsqiantai").attr("href","http://www.ls0736.com/detail?id="+goodsid); 
			$("#sellermain").attr("href","https://store.taobao.com/shop/view_shop.htm?user_number_id="+sellerid); 
			$("#quaninfo").attr("href","https://taoquan.taobao.com/coupon/unify_apply.htm?sellerId="+sellerid+"&amp;activityId="+quanid); 
		}
		function getNowFormatDate() {
		    var date = new Date();
		    var seperator1 = "-";
		    var seperator2 = ":";
		    var month = date.getMonth() + 1;
		    var strDate = date.getDate();
		    if (month >= 1 && month <= 9) {
		        month = "0" + month;
		    }
		    if (strDate >= 0 && strDate <= 9) {
		        strDate = "0" + strDate;
		    }
		    var currentdate = date.getFullYear() + seperator1 + month + seperator1 + strDate
		            + " " + date.getHours() + seperator2 + date.getMinutes()
		            + seperator2 + date.getSeconds();
		    return currentdate;
		}
		
		function updategoods(id,dtitle,goodsid,cid,pic,orgprice,salesnum,sellerid,istmall,quanid,quanprice,updatetime,quantime,commissionjihua,commissionqueqiao,status){
			$("#id").val(id);
			$("#dtitleone").val(dtitle);
			$("#goodsidone").val(goodsid);
			$("#typeid").find("input[value='"+cid+"']").attr("selected",true);
			$("#saleFlag").find("input[value='"+status+"']").attr("selected",true);
			$("#picone").val(pic);
			$("#Org_Priceone").val(orgprice);
			$("#Sales_numone").val(salesnum);
			$("#SellerIDone").val(sellerid);
			if (istmall == 1) {
				$("#isTmall").find("input[value='1']").attr("selected",true);
			} else {
				$("#isTmall").find("input[value='0']").attr("selected",true);
			}
			
			var now = getNowFormatDate();
			var oDate1 = new Date(now);
		    var oDate2 = new Date(quantime);
		    if (oDate1.getTime() > oDate2.getTime()) {
		    	$("#couponFlag").find("input[value='1']").attr("selected",true);
		    } else {
		    	$("#couponFlag").find("input[value='0']").attr("selected",true);
		    }
			$("#couponId").val(quanid);
			$("#couponFaceFee").val(quanprice);
			$("#couponStartDate").val(updatetime);
			$("#couponEndDate").val(quantime);
			if (commissionjihua == 0) {
		    	$("#campaignType").find("input[value='1']").attr("selected",true);
		    	$("#campaignRate").val(commissionqueqiao+"%");
		    } else if (commissionqueqiao == 0) {
		    	$("#campaignType").find("input[value='0']").attr("selected",true);
		    	$("#campaignRate").val(commissionjihua+"%");
		    }
		}
		function updategoodsinfo(){
			var id = $("#id").val();
			var dtitle = $("#dtitleone").val();
			var typeid = $("#typeid").val();
			var pic = $("#picone").val();
			var orgprice = $("#Org_Priceone").val();
			var salesnum = $("#Sales_numone").val();
			var sellerid = $("#SellerIDone").val();
			var isTmall = $("#isTmall").val();
			var saleFlag = $("#saleFlag").val();
		    var couponFlag = $("#couponFlag").val();
			var quanid = $("#couponId").val();
			var quanprice = $("#couponFaceFee").val();
			var Commission_jihua = "";
			var Commission_queqiao = "";
			var campaignType = $("#campaignType").val();
			if (campaignType == 0) {
				Commission_jihua = $("#campaignRate").val();
				Commission_jihua = Commission_jihua.substring(0,Commission_jihua.length-1)
				Commission_queqiao = 0
		    } else if (campaignType == 1) {
		    	Commission_queqiao = $("#campaignRate").val();
		    	Commission_queqiao = Commission_queqiao.substring(0,Commission_queqiao.length-1)
		    	Commission_jihua = 0
		    }
			$.ajax({
				type:"post",
				url:"?s=GoodsCenter/updategoodsinfo",
				data:{'id':id,'dtitle':dtitle,'typeid':typeid,'pic':pic,'orgprice':orgprice,'salesnum':salesnum,'sellerid':sellerid,'isTmall':isTmall,'saleFlag':saleFlag,'couponFlag':couponFlag,'quanid':quanid,'quanprice':quanprice,'Commission_jihua':Commission_jihua,'Commission_queqiao':Commission_queqiao},
				dataType : "json",			
				success : function(data){
					//layer.msg(data['sql'],{time:80000});
					if(data['msg'] == 1){
						layer.msg('操作成功',{time:800},function(){
							location.reload(true);
						});
					}else if(data['msg'] == 2){
						layer.msg('操作失败',{time:800});
					}
				}
			});
		}
		$("#submit-btnjump").click(function(){
			var status = $("#jumpstatus").val();
			var id = $("#urlid").val();
			$.ajax({
				type:"post",
				url:"?s=GoodsCenter/editurl",
				data:{'id':id,'status':status},
				dataType : "json",			
				success : function(data){
					//layer.msg(data['sql'],{time:80000});
					if(data['msg'] == 1){
						layer.msg('操作成功',{time:800},function(){
							location.reload(true);
						});
					}else if(data['msg'] == 2){
						layer.msg('操作失败',{time:800});
					}
				}
			});
		});
	</script>

