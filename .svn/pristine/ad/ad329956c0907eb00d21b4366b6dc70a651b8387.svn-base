@CHARSET "UTF-8";
body{
	background: #fff;
}
.search-box{
	background: url("../img/search-bg.png") top center no-repeat;
	background-size: 100%;
	padding-top:1.6rem;
	padding-bottom:1rem;
}
.search-box .search-content{
	margin-left:10px;
	margin-right:10px;
	display: -webkit-flex;
	background: #fff;
	border-radius: 6px;
	overflow: hidden;
}
.search-content img{
	width:0.6rem;
	height: 0.6rem;
	margin-left:10px;
	margin-top: 0.2rem;
}
.search-content .search-text{
	flex:1;
	font-size:14px;
	line-height: 1rem;
	color:#969696;
	margin-left:5px;
}
.search-content .search-button{
	width:57px;
	font-size:16px;
	text-align: center;
	background: #ffd200;
	color:#211b00;
	line-height: 1rem;
	height:1rem;
}
.jieshao{
	text-align: center;
	color:#fff;
	font-size: 0.46rem;
	margin-top:10px;
}
#saveMoney-box{
	margin-top:15px;
	padding-left:5px;
	padding-right:5px;
}
#saveMoney-box .save-title{
	font-size: 14px;
	color:#646464;
	margin-left:5px;
}
.saveMoney-box{
	display: -webkit-flex;
	margin-top:10px;
}
.saveMoney-box .saveMoney{
	flex:1;
	margin-left:5px;
	margin-right:5px;
}
.saveMoney h2{
	text-align: justify;
	display: -webkit-box !important;
	overflow: hidden;
	text-overflow: ellipsis;
	word-break:break-all;
	-webkit-box-orient:vertical;
	-webkit-line-clamp:2;
	margin-top:6px;
	color:#161616;
	font-size: 14px;
	height:30px;
}
.saveMoney strong{
	font-size: 0.45rem;
}
.save-price{
	color:#ff0e2a
}
.sheng{
	display: -webkit-flex;
}
.sheng img{
	width:40px;
	height:16px;
	margin-right:6px;
}
.sheng span{
	color:#646464;
}
#sort-box{
	margin-left:5px;
	margin-right:5px;
	padding-bottom:10px;
}
#sort-box h2{
	font-size: 14px;
	color:#161616;
	margin-top:10px;
}
#sort-box .sort-title{
	margin-left:5px;
	color:#646464;
}
.sort-box{
	width:100%;
	overflow: hidden;
	display: -webkit-flex;
}
.sort-box .sort{
	margin-left:5px;
	margin-right:5px;
	display: -webkit-flex;
	border:1px solid #efefef;
	justify-content:space-between;
	flex:1;
	border-radius: 3px;
	margin-top:10px;
}
.sort-text{
	margin-left:10px;
}
.sort-text p{
	font-size:12px;
	color:#969696;
	margin-top:5px;
	margin-bottom:10px;
}
.sort img{
	display: block;
	width:48px;
	height:48px;
	margin-right:10px;
	margin-top:5px;
}