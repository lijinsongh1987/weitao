@charset "utf-8";
/* CSS Document */

.listbox{
	background:#fff;
	padding:0.3125rem;
	font-size:16px;
	margin-bottom:0.3125rem;
	overflow:hidden;
	}
.listbox .goodsimg{
	float:left;
	border:#f3f3f3 1px solid
	}
.listbox .goodstext{
	float:left;
	margin-left:10px;
	position:relative;
	}
.goodstext h4{
	overflow: hidden;
 	text-overflow: ellipsis;
 	display: -webkit-box;
 	-webkit-line-clamp: 2;
 	-webkit-box-orient: vertical;
	}
.goodstext h6{
	color:#ff331e
	}
.goodstext .title{
	}
.buttonbox{
	width:100%;
	overflow:hidden;
	}
.buttonbox .button{
	width:6em;
	float:left;
	text-align:center;
	border-radius:6px;
	color:#fff;
	font-size:14px;
	}
.buttonbox .button:hover{
	outline: none;
	text-decoration:none
	}
.buy{
	background:#ff921e;
	}
.bordersubmit{
	background:#38b95c;
	margin-left:10%;
	}
.more{
	text-align:center;
	font-size:16px;
	color:#969696;
	padding-top:0.3125rem;
	}
.popbox{
	width:100%;
	max-width:1024px;
	height:100%;
	background:rgba(0,0,0,0.6);
	position:fixed;
	top:0;
	display:none;
	z-index:9;
	}
.popconten{
	width:288px;
	background:#fff;
	position:fixed;
	top:20%;
	left:50%;
	margin-left:-144px;
	border-radius:6px;
	overflow:hidden;
	padding-bottom:0.3125rem;
	}
.popconten .title{
	width:100%;
	height:3em;
	background:#38b95c;
	line-height:3em;
	text-align:center;
	color:#fff;
	font-size:18px;
	}
.popconten .orderimg{
	font-size:18px;
	width:9em;
	margin:0 auto;
	margin-top:0.625rem;
	margin-bottom:0.3125rem;
	}
.popconten .inputcss{
	display:block;
	width:90%;
	margin:0 auto;
	border-radius:66px;
	border:none;
	background:none;
	line-height:2.6em;
	font-size:16px;
	}
.popconten .ordernumber{
	border:#e4e4e4 1px solid;
	text-indent:1.2em;
	}
.popconten .ordersubmit{
	background:#ff921e;
	color:#fff;
	margin-top:1em;
	}
.popconten .cancel{
	color:#969696
	}
#msg{
	position:fixed;
	z-index:99;
	top:40%;
	width:6em;
	left:50%;
	margin-left:-3em;
	color:#fff;
	text-align:center;
	border-radius:6px;
	font-size:16px;
	line-height:3em;
	background:rgba(0,0,0,0.7);
	display:none;
	}






