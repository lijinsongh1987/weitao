@charset "utf-8";
/* CSS Document */


/*order_list css*/
.listbox{
	background:#fff;
	padding:15px;
	overflow:hidden;
	font-size:16px;
	
	}
.list{
	margin-bottom:10px;
	}
.listbox .goodsimg{
	float:left;
	width:25%;
	border:#f3f3f3 1px solid
	}
.listbox .goodstext{
	width:68%;
	float:right;
	margin-left:15px;
	
	}
.goodstext .title{
	width:100%;
	overflow: hidden;
	
	}
.title h4{
	overflow: hidden;
 	text-overflow: ellipsis;
 	display: -webkit-box;
 	-webkit-line-clamp: 2;
 	-webkit-box-orient: vertical;
 	text-align:left;
	color:#161616;
	float:left;
	width:70%;
	}
.title h6{
	width:30%;
	float:left;
	color:#ff331e;
	text-align:right;
	font-size:16px;
	}
.goodstext .status{
	margin-top:15px;
	width:100%;
	overflow:hidden;
	color:#38b95c;
	
	}
.order_button{
	background:#fff;
	padding:15px;
	margin-top:1px;
	overflow:hidden;
	}
.order_button a{
	padding-left:1em;
	padding-right:1em;
	line-height:2em;
	text-align:center;
	float:right;
	margin-left:0.3125rem;
	text-decoration:none;	
	}
.order_button a:hover{
	text-decoration:none;
	}
.order_button .receipt{
	color:#ff331e;
	border-radius:3px;
	border: solid #ff331e 1px ;
	}
.order_button .logistics{
	color:#646464;
	border-radius:3px;
	border: solid #646464 1px;
	}
.defaultpage{
	width:100%;
	height:100%;
	}
.defaultpage img{
	max-width:140px;
	margin:0 auto;
	margin-top:40%;
	}
.defaultpage p{
	text-align:center;
	font-size:14px;
	color:#8b8b8b;
	margin-top:1em;
	}
.popbox{
	width:100%;
	max-width:1024px;
	height:100%;
	background:rgba(0,0,0,0.6);
	position:fixed;
	top:0;
	display:none;
	z-index:9;
	}

.pop{
	width:288px;
	background:#fff;
	position:fixed;
	top:20%;
	left:50%;
	margin-left:-144px;
	border-radius:6px;
	overflow:hidden;
	padding-bottom:0.625rem;
	padding-top:1rem;
	}

.pop .displayimg{
	width:158px;
	margin:0 auto;
	}
.pop p{
	color:#161616;
	font-size:16px;
	text-align:center;
	}

.no{
	background:#439df9;
	width:80%;
	display:block;
	margin:0 auto;
	color:#fff;
	line-height:2.6em;
	font-size:16px;
	text-align:center;
	border-radius:6px;
	margin-top:1em;
	}

.cancel{
	width:80%;
	display:block;
	margin:0 auto;
	margin-top:1em;
	color:#969696;
	background:none;
	border:none;
	}	

#code{
		display:block;
		margin:0 auto;
		width:80%;
		font-size:16px;
		line-height:2.4em;
		border:#dddddd 1px solid;
		border-radius:6px;
		color:#161616;
		margin-top:1em;
		text-indent:1em;
		}
	
/*logistics css*/

.logistics p{
	font-size:14px;
	color:#646464;
	line-height:1.6em;
	}
.track{
	background:#fff;
	padding:0.3125rem;
	}
.track h4{
	font-size:16px;
	}
.track hr{
	border:#f3f3f3 1px solid; 
	margin-top:0.3125rem;
	margin-bottom:0.3125rem;
	}
.track .msghr{
	border:#f3f3f3 1px solid; 
	margin-top:0;
	margin-bottom:0;
	}
.msgbox{
	overflow:hidden;
	}	
.msgbox .msgimg{
	width:14px; 
	display:block;
	}
.msgbox .imgbox{
	float:left;
	width:14px;
	}	
.imgbox p{
	width:1px;
	background:#dddddd;
	margin-left:6px;
	}	
.msgbox .textbox{
	float:left;
	margin-left:10px;
	}	
.textbox h4{
	line-height:1em; 
	margin-bottom:0.3125rem;
	font-size:16px;
	color:#969696;
	}	
.textbox h6{
	color:#969696;
	font-size:14px;
	}	
.textbox .active{
	color:#38b95c
	}	
	
	
	