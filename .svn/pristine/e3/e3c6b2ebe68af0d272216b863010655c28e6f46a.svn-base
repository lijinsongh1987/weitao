#ps-inf-box{
	background-size: 100%;
	padding-top: 3.6rem;
	position: relative;
	min-height:4.1rem;
}

.zhezhao{
	background: rgba(0,0,0,0.7) url("../img/avatar-bg.png") bottom center no-repeat;
	background-size:100%;
	position: absolute;
	top:0;
	left:0;
	width:100%;
	height:4.1rem;
	z-index: 9;
}
.img{
	position: absolute;
	width:2.4rem;
	height: 2.4rem;
	border-radius: 666px;
	background: #fff;
	overflow: hidden;
	left:50%;
	margin-left:-1.2rem;
	bottom:0;
	z-index: 99
}
.img img{
	width:2.2rem;
	height: 2.2rem;
	border-radius: 66px;
	margin: 0 auto;
	margin-top:0.1rem;
}
#conten .name{
	background: #fff;
	width:100%;
	text-align: center;
	font-size: 16px;
	color: #414141;
	padding-top:10px;
	padding-bottom: 15px;
}
.img-box .order{
	background: #fff;	
}
#conten{
	background: #f3f3f3;
	padding-bottom: 0.3124rem;
}
.order-icon-box{
	background: #fff;
	padding-top:15px;
	padding-bottom:15px;
	display: -webkit-flex;
	border-top:1px solid #f3f3f3;
	border-bottom:1px solid #f3f3f3;
}
.order-icon-box hr{
	height: 100%;
	width: 1px;
	background: #f3f3f3;
}
.order-icon-box .order-icon{
	font-size:12px;
	flex:1;
	color: #414141;
}
.order-icon-box .order-icon:hover{
	text-decoration: none;
}
.order-icon img{
	width:0.7rem;
	margin: 0 auto;
	margin-bottom:5px;
}
.all-order{
	display: -webkit-flex;
	justify-content:space-between;
	padding-top: 15px;
	padding-bottom: 15px;
	background: #fff;	
}
.all-order .icon-img{
	width:17px;
	height: 20px;
	margin:0 10px 0 0.3125rem;
}
.all-order p{
	line-height: 20px;
	font-size: 15px;
	color: #414141;
	flex:1;
}
.all-order .more{
	flex:1;
	
}
.more img{
	width:9px;
	height: 15px;
	margin-right: 0.3125rem;
	float: right;
	margin-top: 2.5px;
}
#moeny-box{
	display: -webkit-flex;
	background: #fff;
	margin-top: 0.3125rem;
	padding:  0.3125rem 0 0.3125rem 0.3125rem;
}
#moeny-box .moeny-icon{
	width:17px;
	height: 20px;
	margin-right: 10px;
}
#moeny-box .moeny{
	flex:1;
}
.moeny .my-moeny{
	justify-content:space-between;
	display: -webkit-flex;
}
.my-moeny p{
	font-size: 15px;
	color: #414141;
}
.my-moeny .withdraw{
	flex:1;
	overflow: hidden;
	margin-bottom: 0.3125rem;
}
.withdraw img{
	width:9px;
	height: 15px;
	margin-right: 0.3125rem;
	margin-left: 10px;
	float: right;
	margin-top: 2.5px;
}
.withdraw span{
	display:block;
	float:right;
	width:5em;
	background:#008aff;
	color: #fff;
	text-align: center;
	line-height: 20px;
}
.moeny-text{
	display: -webkit-flex;
	margin-top: 0.3125rem;
}
.moeny-text .text{
	flex:1;
}
.text h2{
	font-size:20px;
	font-weight: 600;
	color:#ff6d0e;
}
.text span{
	font-size: 12px;
	font-weight: 100;
}
.text p{
	color: #666666;
	font-size: 12px;
	margin-top: 5px;
}
.message-box{
	background: #fff;
	margin-top: 0.3125rem;
	padding-left: 0.3125rem;
}
.message-box .model{
	display: -webkit-flex;
	align-items:center;
	padding-bottom: 0.3125rem;
	padding-top: 0.3125rem;
}
.model img{
	width:0.88rem;
	height:0.88rem;
}
.model p{
	line-height: 0.88rem;
	font-size: 14px;
	margin-left: 10px;
	width:4em;
}
.model a{
	flex:1;
}
.model .more{
	width:9px;
	height: 15px;
	margin-right: 0.3125rem;
	float: right;
	margin-top: 2.5px;
}


